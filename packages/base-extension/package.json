{
  "name": "base-extension",
  "version": "3.0.0",
  "description": "Base extension implementation for Zeplin stylesheet extensions",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "author": {
    "name": "Zeplin",
    "email": "support@zeplin.io",
    "url": "http://zeplin.io"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rm -rf ./dist",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "zeplin-extension-style-kit": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@zeplin/extension-model": "^3.0.3",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  }
}
